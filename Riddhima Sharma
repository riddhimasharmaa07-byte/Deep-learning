import pandas as pd
from sklearn.linear_model import LinearRegression

# Dataset
data = {
    'Area': [1000, 1500, 2000, 2500],
    'Bedrooms': [2, 3, 3, 4],
    'Price': [50, 75, 100, 130]
}

df = pd.DataFrame(data)

X = df[['Area', 'Bedrooms']]
y = df['Price']

model = LinearRegression()
model.fit(X, y)

print("Prediction:", model.predict([[1800, 3]]))
